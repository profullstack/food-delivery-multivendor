<svg width="200" height="80" viewBox="0 0 200 80" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Gradient for cigar body -->
    <linearGradient id="cigarGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#A0522D;stop-opacity:1" />
      <stop offset="70%" style="stop-color:#654321;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3E2723;stop-opacity:1" />
    </linearGradient>
    
    <!-- Gradient for cigar band -->
    <linearGradient id="bandGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FFD700;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FFA500;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF8C00;stop-opacity:1" />
    </linearGradient>
    
    <!-- Shadow filter -->
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="2" flood-color="#000000" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Background circle -->
  <circle cx="100" cy="40" r="35" fill="#1a1a1a" opacity="0.1"/>
  
  <!-- Main cigar body -->
  <ellipse cx="100" cy="40" rx="45" ry="8" fill="url(#cigarGradient)" filter="url(#shadow)"/>
  
  <!-- Cigar texture lines -->
  <line x1="60" y1="35" x2="140" y2="35" stroke="#654321" stroke-width="0.5" opacity="0.6"/>
  <line x1="60" y1="37" x2="140" y2="37" stroke="#8B4513" stroke-width="0.3" opacity="0.4"/>
  <line x1="60" y1="43" x2="140" y2="43" stroke="#8B4513" stroke-width="0.3" opacity="0.4"/>
  <line x1="60" y1="45" x2="140" y2="45" stroke="#654321" stroke-width="0.5" opacity="0.6"/>
  
  <!-- Cigar band -->
  <rect x="75" y="32" width="20" height="16" fill="url(#bandGradient)" rx="2" filter="url(#shadow)"/>
  
  <!-- Band text -->
  <text x="85" y="42" font-family="serif" font-size="6" font-weight="bold" text-anchor="middle" fill="#8B0000">CU</text>
  
  <!-- Cigar tip (left) -->
  <ellipse cx="55" cy="40" rx="3" ry="8" fill="#3E2723"/>
  
  <!-- Cigar end (right) -->
  <ellipse cx="145" cy="40" rx="2" ry="8" fill="#654321"/>
  <circle cx="145" cy="40" r="1.5" fill="#8B4513"/>
  
  <!-- Smoke wisps -->
  <path d="M 147 35 Q 152 30 155 35 Q 158 40 162 35 Q 165 30 168 35" stroke="#D3D3D3" stroke-width="1" fill="none" opacity="0.6"/>
  <path d="M 148 45 Q 153 50 156 45 Q 159 40 163 45 Q 166 50 169 45" stroke="#D3D3D3" stroke-width="0.8" fill="none" opacity="0.4"/>
  
  <!-- Company name -->
  <text x="100" y="65" font-family="serif" font-size="12" font-weight="bold" text-anchor="middle" fill="#3E2723">CigarUnderground.org</text>
</svg>
